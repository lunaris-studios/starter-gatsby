{
  "name": "starter-gatsby",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "author": "Sam Craig <sam@lunaris.io>",
  "dependencies": {
    "gatsby": "^2.24.2",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gatsby-plugin-typegen": "^2.2.1",
    "gatsby-plugin-typescript": "^2.4.14",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^2.5.11",
    "gsap": "^3.4.0",
    "immer": "^7.0.5",
    "libreact": "^2.13.3",
    "npm-check-updates": "^7.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-universal-interface": "^0.6.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/introspection": "1.17.0",
    "@graphql-codegen/typescript": "1.17.0",
    "@graphql-codegen/typescript-operations": "1.17.0",
    "@graphql-codegen/typescript-react-apollo": "1.17.0",
    "@testing-library/jest-dom": "^5.11.1",
    "@types/jest": "^26.0.4",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "jest-styled-components": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "npm-run-all clean:* --parallel",
    "clean:gatsby": "rm -rf .cache/** && rm -rf public/**",
    "clean:debug": "rm -rf ./npm-debug*",
    "clean:modules": "rm -rf ./node_modules package-lock.json",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,md}",
    "generate": "run-s dev generate:graphql-codegen",
    "generate:graphql-codegen": "graphql-codegen --config codegen.yaml",
    "generate:stub-server": "np",
    "serve": "gatsby serve",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "update": "ncu --upgrade && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lunaris-studios/starter-gatsby"
  },
  "bugs": {
    "url": "https://github.com/lunaris-studios/starter-gatsby/isssues"
  }
}
