{
	"name": "starter-gatsby",
	"private": true,
	"description": "A simple starter to get up and developing quickly with Gatsby",
	"version": "1.0.0",
	"author": "Sam Craig <sam@lunaris.io>",
	"scripts": {
		"build": "npm run compile",
		"clean": "npm-run-all clean:* --parallel",
		"clean:debug": "rm -rf ./npm-debug*",
		"clean:gatsby": "rm -rf .cache/** && rm -rf public/**",
		"clean:modules": "rm -rf ./node_modules package-lock.json",
		"compile": "run-s \"compile:*\"",
		"compile:gatsby": "gatsby build",
		"dist": "run-s test:jest compile",
		"dev": "gatsby develop --port=9000",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,md}",
		"generate": "run-s dev generate:graphql-codegen",
		"generate:graphql-codegen": "graphql-codegen --config codegen.yaml",
		"generate:stub-server": "np",
		"lighthouse": "lhci autorun",
		"release": "semantic-release --ci",
		"serve": "gatsby serve",
		"start": "npm run dev",
		"test:jest": "jest --json --outputFile=./target/coverage/.jest-test-results.json",
		"test:jest:watch": "jest --watch",
		"update": "ncu --upgrade && npm install"
	},
	"dependencies": {
		"@paradigmjs/gatsby": "0.0.8",
		"@paradigmjs/protocol": "0.0.13",
		"@paradigmjs/sensors": "0.0.13",
		"@paradigmjs/universal": "0.0.13",
		"@paradigmjs/util": "0.0.13",
		"@reach/router": "^1.3.4",
		"gatsby": "^2.24.33",
		"gatsby-alias-imports": "^1.0.4",
		"gatsby-image": "^2.4.14",
		"gatsby-plugin-catch-links": "^2.3.11",
		"gatsby-plugin-manifest": "^2.4.22",
		"gatsby-plugin-nprogress": "^2.3.10",
		"gatsby-plugin-offline": "^3.2.22",
		"gatsby-plugin-page-creator": "^2.3.20",
		"gatsby-plugin-react-helmet": "^3.3.10",
		"gatsby-plugin-sharp": "^2.6.25",
		"gatsby-plugin-styled-components": "^3.3.10",
		"gatsby-plugin-transition-link": "^1.20.2",
		"gatsby-plugin-typegen": "^2.2.1",
		"gatsby-plugin-typescript": "^2.4.17",
		"gatsby-source-filesystem": "^2.3.24",
		"gatsby-transformer-sharp": "^2.5.12",
		"gsap": "^3.4.2",
		"immer": "^7.0.7",
		"libreact": "^2.13.3",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-redux": "^7.2.1",
		"react-spring": "^9.0.0-rc.3",
		"react-universal-interface": "^0.6.2",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@google/semantic-release-replace-plugin": "^1.0.0",
		"@graphql-codegen/cli": "^1.17.7",
		"@graphql-codegen/introspection": "1.17.7",
		"@graphql-codegen/typescript": "1.17.7",
		"@graphql-codegen/typescript-operations": "1.17.7",
		"@graphql-codegen/typescript-react-apollo": "2.0.5",
		"@lhci/cli": "^0.4.4",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@testing-library/jest-dom": "^5.11.2",
		"@testing-library/react": "^10.4.8",
		"@types/jest": "^26.0.9",
		"@types/react-redux": "^7.1.9",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"husky": "^4.2.5",
		"jest": "^26.2.2",
		"jest-extended": "^0.11.5",
		"jest-styled-components": "^7.0.2",
		"lint-staged": "^10.2.11",
		"npm-check-updates": "^7.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "2.0.5",
		"semantic-release": "^17.1.1",
		"source-map-support": "^0.5.19",
		"ts-jest": "^26.1.4",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.9.7"
	},
	"keywords": [
		"gatsby",
		"typescript"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lunaris-studios/starter-gatsby"
	},
	"bugs": {
		"url": "https://github.com/lunaris-studios/starter-gatsby/isssues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md}": "prettier --write"
	}
}
